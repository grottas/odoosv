<?xml version="1.0" encoding="UTF-8" ?>
<openerp>
    <data>

        <record id="hr_view_sv_employee" model="ir.ui.view">
            <field name="name">hr.view.sv.employee.form</field>
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="hr.view_employee_form"/>
            <field name="arch" type="xml">

				<xpath expr="//field[@name='country_id']" position="before">
					<field name="sv_nombre_dui"/>
					<field name="sv_apellido_dui"/>
					<field name="sv_afp"/>
				</xpath>

            </field>
        </record>
        
        
        
    </data>
</openerp>
